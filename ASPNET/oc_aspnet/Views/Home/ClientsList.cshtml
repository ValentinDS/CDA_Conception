<!DOCTYPE html>
<html>
@{
    ViewBag.Title = "ClientsList";
}
<head>

</head>

<body>
    <h2>ClientsList</h2>

    <p>Voici la liste des clients !</p>
    <br />

    <table>
        <tr>
            <td>Nom</td>
            <td>Age</td>
        </tr>


        @foreach (oc_aspnet.Models.Client client in ViewBag.ListClients as List<oc_aspnet.Models.Client>)
        {
            <tr>
                <td>@client.Name </td>
                <td>@client.Age</td>

            </tr>
        }

    </table>
</body>


</html>